{
    "vetur.format.defaultFormatter.html": "js-beautify-html",
    "vetur.format.defaultFormatter.js": "vscode-typescript",
    "eslint.enable": true,
    "eslint.autoFixOnSave": true,
    "eslint.run": "onType",
    "eslint.options": {
        "extensions": [".js", ".vue"],
    },
    "eslint.validate": [
        "javascript",
        { "language": "vue", "autoFix": true },
        {
          "language": "html",
          "autoFix": true
        }
    ]
}